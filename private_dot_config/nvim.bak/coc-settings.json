{
    "languageserver": {
        "cmake": {
            "command": "cmake-language-server",
            "filetypes": ["cmake"],
            "rootPatterns": [
                "build/"
            ],
            "initializationOptions": {
                "buildDirectory": "build"
            }
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
  "jedi.executable.command": "/usr/bin/jedi-language-server",
  "texlab.build.onSave": true,
  "texlab.rootDirectory": ".",
  "texlab.build.args": [""],
  "texlab.chktex.onEdit": true,
  "texlab.chktex.onOpenAndSave": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.telemetry.enable": false,
  "Lua.workspace.library": ["/usr/share/nvim/runtime/lua", "/usr/share/nvim/runtime/lua/lsp", "/usr/share/awesome/lib"],
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": ["vim", "awesome", "client", "root", "screen"],
  "clangd.inlayHints.enable": false
}
